<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/navbar_background"
    tools:layout_editor_absoluteX="-5dp"
    tools:layout_editor_absoluteY="777dp"
    android:id="@+id/header"
    android:layout_gravity="top"
    >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:padding="5dp"
        android:id="@+id/buttons_header">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/logout"
            app:tint="@color/white"
            app:layout_constraintStart_toStartOf="@id/buttons_header"
            app:layout_constraintTop_toTopOf="@id/buttons_header"
            app:layout_constraintEnd_toEndOf="@+id/buttons_header"
            app:layout_constraintBottom_toBottomOf="@id/buttons_header"
            android:id="@+id/logoutIcon"
            app:layout_constraintHorizontal_bias="0.95"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/support"
            app:tint="@color/white"
            app:layout_constraintStart_toStartOf="@id/buttons_header"
            app:layout_constraintTop_toTopOf="@id/buttons_header"
            app:layout_constraintEnd_toEndOf="@+id/buttons_header"
            app:layout_constraintBottom_toBottomOf="@id/buttons_header"
            android:id="@+id/supportIcon"
            app:layout_constraintHorizontal_bias="0.05"
            />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>